{
  "name": "typeorm-logger-adaptor",
  "version": "1.0.2",
  "description": "Logger adaptors for TypeORM",
  "author": {
    "name": "KOMIYA Atsushi",
    "url": "https://github.com/komiya-atsushi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komiya-atsushi/typeorm-logger-adaptor.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc -p tsconfig.json",
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint --fix --ext .ts ./",
    "test": "docker-compose up -d && jest --coverage --collect-coverage-from='src/**/*.ts'"
  },
  "keywords": [
    "typescript",
    "typeorm",
    "logger",
    "winston",
    "bunyan"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "bunyan": "^1.8.5",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "mysql2": "^2.2.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.3",
    "winston": "^3.0.0"
  }
}
